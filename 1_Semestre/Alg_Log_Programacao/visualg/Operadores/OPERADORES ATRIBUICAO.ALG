Algoritmo "Operadores Atribuicao"

Var
lado: inteiro
valor_pi: real
string: caracter
char_caract: caracter
area: real

Inicio
lado <- 2;
valor_pi <- 3,14;
string <- "texto";
char_caract <- "A";
area <- 2;
area <- area*2;

Escreval("lado = ", lado)
Escreval("valor_pi = ", valor_pi)
Escreval("string = ", string)
Escreval("char_caract = ", char_caract)
Escreval("area = ", area)




Fimalgoritmo