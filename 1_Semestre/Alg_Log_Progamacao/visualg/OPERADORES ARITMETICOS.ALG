Algoritmo "Operadores Aritmeticos"

Var
n1, n2: real
soma:real
sub: real
divi:real
mult: real

Inicio
n1 <- 5.8;
n2 <- 3.2;

//adição
soma <- n1 + n2;
Escreval("A soma é = ", soma:2:1)

//subtração
sub <- n1 - n2;
Escreval("A sub = ", sub:18:16)

//multiplicação
mult <- n1 * n2;
Escreval("A mult = ", mult)

//divisão
divi <- n1 / n2;
Escreval("A divi = ", divi:18:16)





Fimalgoritmo